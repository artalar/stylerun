{
  "version": "0.0.1-rc1",
  "name": "stylerun",
  "description": "Stylerun is natural and performant way to style your (P?)React application",
  "author": "artalar",
  "license": "MIT",
  "sideEffects": false,
  "source": "src/index.ts",
  "exports": {
    "types": "./build/index.d.ts",
    "require": "./build/index.js",
    "default": "./build/index.module.js"
  },
  "main": "build/index.js",
  "module": "build/index.module.js",
  "types": "build/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "style",
    "stylerun",
    "styled-components"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && npm run size",
    "build": "microbundle",
    "test": "node src/index.test.ts",
    "size": "size-limit"
  },
  "browserslist": [
    "last 1 year"
  ],
  "minify": {
    "mangle": false
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/artalar/stylerun.git"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2
  },
  "size-limit": [
    {
      "path": "build/index.js",
      "limit": "1 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "microbundle": "^0.15.1",
    "prettier": "^3.0.0",
    "size-limit": "^8.2.6",
    "typescript": "^3.9.10"
  }
}
